

<div class="d-flex justify-content-center align-items-center">
<div class="card card_profile">
  <%= simple_form_for @consultation, url: patient_consultation_path(patient_id: @patient.id, id: @consultation.id) do |f| %>
    <div tabindex="-1">
      <h1>Diagnóstico</h1>
      <%= f.text_area :diagnostic, class: "form-control", placeholder: "Ingrese el diagnóstico aquí" %>
    </div>
    <%= f.button :submit, "Crear consulta", class: "btn btn-primary" %>
  <% end %>
</div>
</div>
